{
  "name": "opis-manager",
  "version": "0.0.0",
  "private": true,
  "packageManager": "npm@11.6.2",
  "scripts": {
    "ng": "ng",
    "prestart": "node set-env.js",
    "start": "ng serve --port 1000 --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "test": "ng test",
    "test:coverage": "ng test --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "dependencies": {
    "@angular/common": "21.0.3",
    "@angular/compiler": "21.0.3",
    "@angular/core": "21.0.3",
    "@angular/forms": "21.0.3",
    "@angular/platform-browser": "21.0.3",
    "@angular/router": "21.0.3",
    "@angular/cdk": "21.0.5",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "chart.js": "4.3.0",
    "ng2-charts": "8.0.0",
    "gsap": "3.13.0",
    "dotenv": "17.2.3"
  },
  "devDependencies": {
    "@angular/cli": "21.0.2",
    "@angular/build": "21.0.2",
    "@angular/compiler-cli": "21.0.3",
    "@angular-eslint/builder": "21.0.1",
    "@angular-eslint/eslint-plugin": "21.0.1",
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-eslint/schematics": "21.0.1",
    "@angular-eslint/template-parser": "21.0.1",
    "angular-eslint": "21.0.1",
    "typescript": "5.9.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "vitest": "4.0.8",
    "jsdom": "27.1.0"
  }
}